  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:injurka/nord-u.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/injurka"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "rollup": "rollup -c",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "lint": "pnpm lint:ts && pnpm lint:scss",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}' && stylelint --fix '**/*.scss'",
    "format": "prettier -c \"./**/*.{ts,tsx,json,scss,md}\"",
    "format:fix": "prettier -w \"./**/*.{ts,tsx,json,scss,md}\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "license": "ISC",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn tsc --noEmit",
      "eslint --fix --ignore-path .gitignore"
    ],
    "*.scss": [
      "stylelint --fix --ignore-path .gitignore"
    ],
    "*.(ts,tsx,json,scss,md)": [
      "prettier -c \"./**/*.{ts,tsx,json,scss,md}\""
    ]
  },
